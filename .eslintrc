{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "object-curly-newline": ["error", { "multiline": true, "minProperties": 5 }]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  }
}
