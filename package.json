  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "bootstrap": "^3.3.5",
    "eslint": "^1.5.1",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.5.0",
    "file-loader": "^0.8.4",
    "jasmine": "^2.3.2",
    "jsx-loader": "^0.13.2",
    "path": "^0.12.7",
    "react-bootstrap": "^0.27.3",
    "react-dom": "^0.14.2",
    "uglify-js": "^2.4.24",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
